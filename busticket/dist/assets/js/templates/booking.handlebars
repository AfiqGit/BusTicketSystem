<div class="wrapper">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="card-box">
                    <h4 class="mt-0 header-title">All Bookings</h4>
                    <p class="text-muted font-14 mb-3">
                        Your awesome text goes here.Your awesome text goes here.
                    </p>
                    <div class="responsive-table-plugin">
                        <div class="table-rep-plugin">
                            <div class="table-responsive">
                                <table class="table table-striped mb-0">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Ticket ID</th>
                                            <th>User ID</th>
                                            <th>Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each bookings}}
                                        <tr>
                                            <td>{{id}}</td>
                                            <td>{{ticketid}}</td>
                                            <td>{{userid}}</td>
                                            <td>{{status}} <a href="#booking/flipstatus/{{id}}">flip</a></td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>